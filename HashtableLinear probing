#include <iostream>
using namespace std;
#define size 10
int table[size];
void initt()
{
}
for(int i=0;i<size;i++)
{
table[i] =-1;
}
int hashfunction(int key)
{
}
return key%size;
void insert(int key)
{
int index=hashfunction(key);
while(table[index]!=-1)
{
index=(index+1)%size;
}
table[index]=key;
cout<<"Inserted key:"<<key<<", index:"<<index<<endl;
}
void deletee(int key)
{
int flag =0;
int index=hashfunction(key);
int start=index;
while(table[index]!=-1)
{
if(table[index]==key)
{
}
table[index] =-1;
cout<<key<<" deleted!! at index: "<<index<<endl;
flag =1;
break;
index=(index+1)%size;
if(start==index)
}
{
}
break;
}
if(!flag)
{
cout<<key<<" not found!!"<<endl;
}
}
void display()
{
for(int i=0;i<size;i++)
{
cout<<"keys["<<i<<"]: "<<table[i]<<endl;
}
}
bool search(int key)
{
int index=hashfunction(key);
int start=index;
while(table[index]!=-1)
{
if(table[index]==key)
{
}
cout<<key<<" found at index:"<<index<<endl;
return true;
}
index=(index+1)%size;
if(index==start)
{
break;
}
}
cout<<key<<" Not found"<<endl;
return false;
}
int main()
{
initt();
int choice, key, keyy;
while(true)
{
cout << "\n--- Hash Table Operations ---\n";
cout << "1. Insert key\n";
cout << "2. Display table\n";
cout << "3. Search key\n";
cout << "4. Exit\n";
cout<<"5. Delete\n";
cout << "Enter your choice: ";
cin >> choice;
switch(choice)
{
case 1:
cout << "Enter key to insert: ";
cin >> key;
insert(key);
break;
case 2:
display();
break;
case 3:
cout << "Enter key to search: ";
cin >> key;
search(key);
break;
case 4:
cout << "Exiting...\n";
return 0;
case 5:
cout << "Enter key to delete: ";
cin >> keyy;
deletee(keyy);
break;
default:
cout << "Invalid choice! Try again.\n";
}
}
return 0;
}
